// zapolnenie massiva rand chislami


using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication5
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] mas = new int[400];
            Random r = new Random();
            for (int i = 0; i < mas.Length; i++)
            {
                mas[i] = r.Next(0, 101);
                Console.Write(mas[i] + " ");
            }
            Console.Read();

        }
    }
}

